<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.AddConvictionController">
   <children>
      <TextField fx:id="releaseDate" layoutX="40.0" layoutY="51.0" promptText="release date" />
      <TextField fx:id="convictionDate" layoutX="40.0" layoutY="104.0" promptText="conviction date" />
      <TextField fx:id="sentence" layoutX="40.0" layoutY="151.0" promptText="sentence" />
      <TextField fx:id="prisonerID" layoutX="40.0" layoutY="200.0" promptText="ID of prisoner" />
      <Button layoutX="82.0" layoutY="264.0" mnemonicParsing="false" onAction="#addConviction" text="Add conviction" />
   </children>
</AnchorPane>
